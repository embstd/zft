<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/relativeLayoutmain" 
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:background="@drawable/newbg"
 >
    <TextView
android:id="@+id/flag1"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:layout_marginTop="25px"
></TextView>





<ImageView 
android:id="@+id/main_iv1" 
android:src="@drawable/zft_main_1" 
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_below="@+id/flag1"
android:layout_marginLeft="45px"

></ImageView>

<TextView
android:id="@+id/main_tv1" 
android:text="违章查处"
android:textSize="18px"
android:textColor="#000000"
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_below="@+id/main_iv1"
android:layout_marginLeft="48px"
></TextView>








<ImageView 
android:id="@+id/main_iv2" 
android:src="@drawable/zft_main_2" 
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_centerHorizontal="true"
android:layout_below="@+id/flag1"
android:layout_toRightOf="@+id/main_iv1"
android:layout_marginLeft="55px"
></ImageView>

<TextView
android:id="@+id/main_tv2" 
android:text="案件办理"
android:textSize="18px"
android:textColor="#000000"
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_centerHorizontal="true"
android:layout_below="@+id/main_iv2"
android:layout_toRightOf="@+id/main_iv1"
android:layout_marginLeft="58px"
></TextView>







<ImageView 
android:id="@+id/main_iv3" 
android:src="@drawable/zft_main_3" 
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_below="@+id/main_tv1"
android:layout_marginLeft="45px"
android:layout_marginTop="45px"
></ImageView>

<TextView
android:id="@+id/main_tv3" 
android:text="历史记录"
android:textSize="18px"
android:textColor="#000000"
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_below="@+id/main_iv3"
android:layout_marginLeft="48px"
></TextView>









<ImageView 
android:id="@+id/main_iv4" 
android:src="@drawable/zft_main_4" 
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_below="@+id/main_tv2"
android:layout_toRightOf="@+id/main_iv3"
android:layout_marginLeft="55px"
android:layout_marginTop="45px"
></ImageView>

<TextView
android:id="@+id/main_tv4" 
android:text="快速处置"
android:textSize="18px"
android:textColor="#000000"
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_below="@+id/main_iv4"
android:layout_toRightOf="@+id/main_iv3"
android:layout_marginLeft="58px"
></TextView>











<ImageView 
android:id="@+id/main_iv5" 
android:src="@drawable/zft_main_5" 
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_below="@+id/main_tv3"
android:layout_marginLeft="45px"
android:layout_marginTop="45px"
></ImageView>

<TextView
android:id="@+id/main_tv5" 
android:text="行政强制"
android:textSize="18px"
android:textColor="#000000"
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_below="@+id/main_iv5"
android:layout_marginLeft="48px"
></TextView>










<ImageView 
android:id="@+id/main_iv6" 
android:src="@drawable/zft_main_6" 
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_below="@+id/main_tv4"
android:layout_toRightOf="@+id/main_iv5"
android:layout_centerHorizontal="true"
android:layout_marginLeft="55px"
android:layout_marginTop="45px"
></ImageView>

<TextView
android:id="@+id/main_tv6" 
android:text="系统设置"
android:textSize="18px"
android:textColor="#000000"
android:layout_height="wrap_content" 
android:layout_width="wrap_content" 
android:layout_below="@+id/main_iv6"
android:layout_toRightOf="@+id/main_iv5"
android:layout_centerHorizontal="true"
android:layout_marginLeft="58px"
></TextView>
</RelativeLayout>
